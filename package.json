{
  "name": "movies-app",
  "version": "1.0.0",
  "description": "movie app",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run dev\" \"npm run start -C ./server\"",
    "dev:*": "webpack-dev-server --mode development --config ./webpack",
    "dev:client": "webpack-dev-server --mode development --config ./webpack/webpack.config.dev.js",
    "dev:ssr:server": "cross-env NODE_ENV=development node ./ssr/index.js",
    "test": "npm run test",
    "build:client": "webpack --mode production --config ./webpack/webpack.config.prod.js",
    "build:ssr:server": "webpack --mode production --config ./webpack/webpack.config.server.js",
    "start:server": "npm run start -C ./server",
    "lint": "eslint --ext .ts,.tsx --fix src",
    "format": "prettier --write \"./**/*.(ts|tsx)\" "
  },
  "keywords": [
    "movie"
  ],
  "author": "Joe",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "@reduxjs/toolkit": "^1.8.2",
    "@types/react-redux": "^7.1.24",
    "axios": "^0.27.2",
    "clsx": "^1.1.1",
    "concurrently": "^7.2.2",
    "express": "^4.18.1",
    "formik": "^2.2.9",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-redux": "^8.0.2",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.17.12",
    "@babel/plugin-transform-runtime": "^7.18.9",
    "@babel/preset-env": "^7.17.12",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@types/axios": "^0.14.0",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/webpack-hot-middleware": "^2.25.6",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "dotenv-webpack": "^7.1.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "html-webpack-plugin": "^5.5.0",
